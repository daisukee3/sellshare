= link_to tweet_path(tweet) do
  .card
    .card_image
    .card_content
      .card_title
        = tweet.content
      .card_heart
        = image_tag 'heart.svg'
        %span 23
      .card_detail
        = image_tag tweet.user.avatar_image
        %div
          %p= tweet.user.account
          %p= tweet.display_created_at